{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "incremental": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
