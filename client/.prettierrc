{
  "trailingComma": "all",
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)$",
    "^react-dom$",
    "<THIRD_PARTY_MODULES>",
    "^(?:[./@]*routes)(/.*|$)",
    "^(?:[./@]*components)(/.*|$)",
    "^(?:[./@]*hooks)(/.*|$)",
    "^(?:[./@]*utils)(/.*|$)",
    "^(?:[./@]*shared)(/.*|$)",
    "^(?:[./@]*localization)(/.*|$)",
    "^[./]",
    "^(?:[./@]*styles)(/.*|$)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}
